{
  "backend": "glx",
  "blur-background-exclude":
  [
    "window_type = 'dock'",
    "window_type = 'desktop'"
  ],
  "blur-kern": "3x3box",
  "blur-method": "dual_kawase",
  "blur-strength": 5,
  "corner-radius": 18,
  "detect-client-opacity": true,
  "detect-rounded-corners": true,
  "detect-transient": true,
  "fade-in-step": 0.03,
  "fade-out-step": 0.03,
  "fading": true,
  "frame-opacity": 1,
  "glx-no-stencil": true,
  "inactive-opacity": 1,
  "inactive-opacity-override": false,
  "log-level": "warn",
  "mark-ovredir-focused": true,
  "mark-wmwin-focused": true,
  "rounded-corners-exclude":
  [
    "window_type = 'dock'",
    "window_type = 'desktop'"
  ],
  "shadow": true,
  "shadow-exclude":
  [
    "name = 'Notification'",
    "class_g = 'Conky'"
  ],
  "shadow-offset-x": -7,
  "shadow-offset-y": -7,
  "shadow-radius": 7,
  "use-damage": true,
  "vsync": true,
  "wintypes":
  {
    "dnd":
    {
      "shadow": false
    },
    "dock":
    {
      "clip-shadow-above": true,
      "shadow": false
    },
    "dropdown_menu":
    {
      "opacity": 0.8
    },
    "popup_menu":
    {
      "opacity": 0.8
    },
    "tooltip":
    {
      "fade": true,
      "focus": true,
      "full-shadow": false,
      "opacity": 0.75,
      "shadow": true
    }
  }
}